{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parser":"babel-eslint",
  // "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "requireConfigFile": false
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/no-webpack-loader-syntax": "off"
  }
}
